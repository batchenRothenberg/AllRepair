BUGGY_FILENAME=479-A-18240880_BUGGY
CORRECT_FILENAME=479-A-18240906_CORRECT
MAIN_FILENAME=MAIN
OUTPUT_FILENAME=479-A-18240880-18240906
CC=gcc
CFLAGS=-fno-optimize-sibling-calls -fno-strict-aliasing -fno-asm -std=c99
LDFLAGS=-lm -s -O2
all:
	$(CC) $(CFLAGS) -c $(BUGGY_FILENAME).c -o $(BUGGY_FILENAME).o
	$(CC) $(CFLAGS) -c $(CORRECT_FILENAME).c -o $(CORRECT_FILENAME).o
	$(CC) $(CFLAGS) -c $(MAIN_FILENAME).c -o $(MAIN_FILENAME).o
	$(CC) $(BUGGY_FILENAME).o $(CORRECT_FILENAME).o $(MAIN_FILENAME).o -o $(OUTPUT_FILENAME) $(LDFLAGS)
.PHONY: clean
clean:
	rm -f *.o $(OUTPUT_FILENAME)
